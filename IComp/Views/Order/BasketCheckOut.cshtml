}

<div class="container">
    <div class="row">
        <div class="col-lg-7 col-md-12">
            <div class="order-content">
                @(Html.Partial("_OrderBasketPartial", Model.Basket))
            </div>
        </div>
        <div class="col-lg-1 col-md-12">
            </div>
        <div class="col-lg-4 col-md-12">
            @(Html.Partial("_BasketOrderPartial",Model.Order))
        </div>
    </div>
</div>